:  ENDIF
:  SET &LINE# = GET_PROCESS_LINE(&HND#)
:  SET &FIND# = STR_MATCH(&LINE#, "*Start response pattern*")
:  IF &FIND# EQ "Y"
:    SET &FLAG# = 1
:  ENDIF
:ENDPROCESS
:CLOSE_PROCESS &HND#

:PRINT NEW IP ADDRESS : &NEW_IP_ADDRESS#
:IF &NEW_IP_ADDRESS# = "0.0.0.0"
:  PRINT ip  "0.0.0.0"
:  PRINT Workflow canceled
:  SET &JOBNR# = SYS_ACT_ME_NR()
:  SET &STATUS# = CANCEL_UC_OBJECT(&JOBNR#)
:ENDIF

:PUBLISH &NEW_IP_ADDRESS#, , "TOP"
:PUT_VAR_COL &VARA_NAME_TICKET#, "NEW_IP_ADDRESS#", 1, "&NEW_IP_ADDRESS#"
]]></post_process>
