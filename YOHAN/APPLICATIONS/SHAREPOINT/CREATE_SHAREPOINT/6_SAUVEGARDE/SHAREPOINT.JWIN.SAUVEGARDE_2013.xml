   <scripts>
      <row>
         <process><![CDATA[@ECHO OFF

ECHO Lancement de la sauvegarde du site &DNS_SITE#.&DNS_DCP_ZONE#
powershell -executionpolicy bypass -file &DOSSIER#\&SCRIPT# -site &DNS_SITE#.&DNS_DCP_ZONE#
SET retcode=%errorlevel%
ECHO Code retour script PS1 = %retcode%
IF NOT %ERRORLEVEL% == 0 goto :retcode
]]></process>
      </row>
      <row>
         <pre_process><![CDATA[:PUT_ATT HOST=&HOST_SHAREPOINT_PRIMARY#
:PUT_ATT LOGIN="LOGIN.SHAREPOINT.INSTALL_&ENVIRONNEMENT#"
!:PUT_ATT ARCHIVE_KEY1=&SITE#]]></pre_process>
      </row>
