      <row>
         <process><![CDATA[:SWITCH &$PLATFORM#
:CASE "WINDOWS"
cd &PATH_UTILITY#
echo *************************************
echo execution de la reorganization, client &CLIENTNR# :
echo ucybdbre -B -S"&CLIENTNR#"
echo *************************************
echo.
ucybdbre -B -S"&CLIENTNR#"

@set retcode=%errorlevel%
@if NOT %ERRORLEVEL% == 0 goto :retcode

:CASE "UNIX"

cd &PATH_UTILITY#
echo "*************************************"
echo "execution de la reorganization, client &CLIENTNR# :"
echo "./ucybdbre -B -S&CLIENTNR#"
echo "*************************************"
echo ""
./ucybdbre -B -S"&CLIENTNR#"
rc=$?

exit $rc

:OTHER

:ENDSWITCH
]]></process>
