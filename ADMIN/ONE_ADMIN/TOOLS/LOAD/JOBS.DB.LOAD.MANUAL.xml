   <scripts>
      <row>
         <process><![CDATA[:SET &runid# = SYS_ACT_RESTART_ME_NR()
:PRINT "Load fichier &FILETRANS# dans client &DCLIENT# en mode &MODE#"

:SWITCH &$PLATFORM#
:CASE "WINDOWS"
cd &PATH_UTILITY#
set rc=0

echo *************************************
echo Load fichier &FILETRANS# dans client &DCLIENT# en mode &MODE# :
echo ucybdbld -B -C&DCLIENT# -E&MODE# -X&FILETRANS#
echo *************************************
echo.
ucybdbld -B -C&DCLIENT# -E&MODE# -X&FILETRANS#
set rc=%errorlevel%

@set retcode=%rc%
@if NOT %ERRORLEVEL% == 0 goto :retcode

:CASE "UNIX"
cd &PATH_UTILITY#

echo "*************************************"
echo "Load fichier &FILETRANS# dans client &DCLIENT# en mode &MODE# :"
echo "./ucybdbld -B -C&DCLIENT# -E&MODE# -X&FILETRANS#"
echo "*************************************"
echo ""
./ucybdbld -B -C&DCLIENT# -E&MODE# -X&FILETRANS#
rc=$?

exit $rc
:OTHER

:ENDSWITCH
]]></process>
      </row>
      <row>
         <pre_process><![CDATA[:PUT_ATT LOGIN = &LOGIN#
!:INC JOBI.CLIENT_WITHOUT_AUDIT, NOFOUND=IGNORE
:INC JOBI.UNIX_SHELL, NOFOUND=IGNORE
!:INC JOBI.ALIAS, NOFOUND=IGNORE]]></pre_process>
      </row>
