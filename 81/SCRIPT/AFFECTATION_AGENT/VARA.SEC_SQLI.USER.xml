         <type>SQLI_SEC</type>
         <sql><![CDATA[SELECT 
  o.OH_Client,
  o.OH_NAME,
  u.USR_EMail1
from 
  OH o
  inner join USR u on o.OH_Idnr=u.USR_OH_Idnr
where 
  o.OH_Otype='USER'
  and u.USR_EMail1<>''
  and o.OH_Client = ?]]></sql>
