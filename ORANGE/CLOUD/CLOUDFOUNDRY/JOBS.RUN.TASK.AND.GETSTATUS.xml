   <scripts>
      <row>
         <post_process><![CDATA[:P &TASK_GUID#
:P &TASK_NAME#
:P &TASK_STATE#
:P &TASK_MESG_FAILURE#

:SET &ITER# = 0
:SET &TIMEOUT# = "TRUE"

:SET &OBJ_VERIF_TASK# = "JOBS.GET.TASK.SCRI"

:WHILE &ITER# < &LOOP_NB#
:  P "Iteration : &ITER#"
:  P "DÃ©lai entre chaque iteration : &LOOP_DELAY# s"
:  WAIT &LOOP_DELAY#
:  P "Active l'objet &OBJ_VERIF_TASK#"
:  SET &RUNID# = ACTIVATE_UC_OBJECT(&OBJ_VERIF_TASK#, WAIT, , , , PASS_VALUES)
:  SET &TASK_STATE# = GET_PUBLISHED_VALUE(&RUNID#, TASK_STATE#)
:  SET &TASK_MESG_FAILURE# = GET_PUBLISHED_VALUE(&RUNID#, TASK_MESG_FAILURE#)
:  SET &OLD_TOKEN# = &ACCESS_TOKEN#
:  PSET &ACCESS_TOKEN# = GET_PUBLISHED_VALUE(&RUNID#, ACCESS_TOKEN#)
! :  DEBUG TOKEN
:  IF &OLD_TOKEN# <> &ACCESS_TOKEN#
:    p "NEW TOKEN set to &ACCESS_TOKEN#"
:  ELSE
:    p "Keeping OLD TOKEN &ACCESS_TOKEN#"
:  ENDIF
! :  END DEBUG TOKEN
:  P &TASK_STATE#
:  P &TASK_MESG_FAILURE#
:  SWITCH &TASK_STATE#
:    CASE "SUCCEEDED"
:    P "TASK OK : QUIT"
! :    STOP NOMSG, 0, "TASK SUCCEEDED"
! :    MODIFY_STATE RETCODE = 1900
! :    MODIFY_STATE STATUS_TEXT = "TASK SUCCEEDED"
! Change l'iteration pour forcer la sortie de la boucle
:    SET &ITER# = &LOOP_NB#
:    SET &TIMEOUT# = "FALSE"
:    CASE "FAILED"
:    P "TASK KO : QUIT"
:    P &TASK_MESG_FAILURE#
! :    STOP MSG, 50, "TASK FAILED : &TASK_MESG_FAILURE#"
:    MODIFY_STATE RETCODE = 50
:    MODIFY_STATE STATUS_TEXT = "TASK FAILED : &TASK_MESG_FAILURE#"
! Change l'iteration pour forcer la sortie de la boucle
:    SET &ITER# = &LOOP_NB#
:    SET &TIMEOUT# = "FALSE"
:    CASE "RUNNING"
:    P "TASK RUNNING : CONTINUE"
:  ENDSWITCH
:  SET &ITER# = &ITER# + 1
:ENDWHILE

! :STOP MSG, 51, "Nombre d'essai max atteint &ITER#"
! :EXIT 10

:IF &TIMEOUT# = "TRUE"
:  MODIFY_STATE RETCODE = 10
:  MODIFY_STATE STATUS_TEXT = "Nombre d'essai max atteint &ITER#"
:ENDIF]]></post_process>
      </row>
