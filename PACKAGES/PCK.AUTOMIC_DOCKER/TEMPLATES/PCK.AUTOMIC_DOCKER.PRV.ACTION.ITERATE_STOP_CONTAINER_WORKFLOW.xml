      <row>
         <process><![CDATA[:            SET &LEN# = LENGTH(&PROCESSED_CONTAINER#[])
:            SET &VAR# = 1
:            WHILE &VAR# LE &LEN#
:              SET &VAR# = FORMAT(&VAR#,"0")
:              PRINT 'Stopped container id &PROCESSED_CONTAINER#[&VAR#]'
:              SET &VAR# = &VAR#+ 1
:            ENDWHILE
]]></process>
