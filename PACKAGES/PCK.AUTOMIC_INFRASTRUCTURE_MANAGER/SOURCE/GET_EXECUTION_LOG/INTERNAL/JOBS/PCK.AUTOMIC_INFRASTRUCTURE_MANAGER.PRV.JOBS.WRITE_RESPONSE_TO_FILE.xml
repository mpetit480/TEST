   <scripts>
      <row>
         <pre_process><![CDATA[:SET &CONN_ENDPOINT_URL#=&UC4RB_EM_REST_END_POINT#

:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.PREPARE_RA_JOB
:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.SET_CONNECTION_AUTH_BASIC

!parse execution_log varible to prepare resource variable
:SET &LEN#=STR_LENGTH(&UC4RB_EM_REST_END_POINT#)
:SET &START_INDEX#=&LEN#+1
:SET &RESOURCE#= SUBSTR(&UC4RB_EM_EXECUTION_LOG_LINK#,&START_INDEX#)

!get host
:SET &AGENT# = GET_ATT(HOST)

!get the agent working dir
:SET &UC4RB_AGENT_WORKING_DIR# = GET_VAR('UC_EX_PATH_TEMP', &AGENT#)

!compare about the input of the value
: IF &UC4RB_EM_SAVE_AS# EQ ""
:    SET &UC4RB_EM_SAVED_LOG_FILE_PATH# =STR_CAT(&UC4RB_AGENT_WORKING_DIR#,"log-&$RUNID#.txt")
:ELSE
:    SET &UC4RB_EM_SAVED_LOG_FILE_PATH#=&UC4RB_EM_SAVE_AS#
:ENDIF



]]></pre_process>
      </row>
      <row>
         <post_process><![CDATA[!Publishing Saved Log File Path
:PUBLISH &UC4RB_EM_SAVED_LOG_FILE_PATH#,,WORKFLOW]]></post_process>
      </row>
   </scripts>
