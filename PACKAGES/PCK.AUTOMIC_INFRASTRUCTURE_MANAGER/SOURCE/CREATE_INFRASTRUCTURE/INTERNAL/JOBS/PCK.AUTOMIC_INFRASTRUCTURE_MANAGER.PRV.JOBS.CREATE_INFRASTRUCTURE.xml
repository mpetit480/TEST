   <scripts>
      <row>
         <process><![CDATA[:PSET &SUCCESS_STATUS# = ""
:PSET &FAILURE_STATUS# = ""

: IF &UC4RB_EM_SUCESS# = ""	
:   SET &SUCCESS_STATUS# = "Completed"
: ELSE
:   SET &SUCCESS_STATUS# = &UC4RB_EM_SUCESS#
: ENDIF

: IF &UC4RB_EM_FAILURE# = ""	
:   SET &FAILURE_STATUS# = "Error,Cancelled"
: ELSE
:   SET &FAILURE_STATUS# = &UC4RB_EM_FAILURE#
: ENDIF



:DEFINE &SPLITED_SUCCESS_ARRAY#,string,30
:FILL &SPLITED_SUCCESS_ARRAY#[] = STR_SPLIT(&SUCCESS_STATUS#,",")
:SET &STATUS_COUNT# = LENGTH(&SPLITED_SUCCESS_ARRAY#[],SIZE)

:SET &COUNT#=1
:WHILE &COUNT# <= &STATUS_COUNT#
:  IF "0" NE STR_FIND(&FAILURE_STATUS#,&SPLITED_SUCCESS_ARRAY#[&COUNT#])
:     P "ERROR: Job could not be completed successfully as the sucess status and failure status have some common values."
:     STOP MSG, 50, "ERROR: sucess status and failure status should not be same."
:  ENDIF
:  SET &COUNT#=&COUNT#+1
:ENDWHILE

:PUBLISH &SUCCESS_STATUS#,,WORKFLOW
:PUBLISH &FAILURE_STATUS#,,WORKFLOW]]></process>
      </row>
      <row>
         <pre_process><![CDATA[:SET &CONN_ENDPOINT_URL#=&UC4RB_EM_REST_END_POINT#

:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.PREPARE_RA_JOB
:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.SET_CONNECTION_AUTH_BASIC

!Escaping Inputs

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_ENV_NAME#
:INC PCK.AUTOMIC_INFRASTRUCTURE_MANAGER.PUB.INCLUDE.ESCAPE_ARG_VALUE
:SET &EM_ENV_NAME#=&UC4RB_ESCAPED_STRING#

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_BCKEND_PROVIDER#
:INC PCK.AUTOMIC_INFRASTRUCTURE_MANAGER.PUB.INCLUDE.ESCAPE_ARG_VALUE
:SET &EM_BCKEND_PROVIDER#=&UC4RB_ESCAPED_STRING#

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_INFRA_PRVDR#
:DEFINE  &EM_INFRA_PRVDR_ARRAY#, string, 30
:FILL &EM_INFRA_PRVDR_ARRAY#[] = STR_SPLIT(&UC4RB_ESCAPED_STRING#,",")
:SET &EM_INFRA_STRING# = STR_CAT('"',&EM_INFRA_PRVDR_ARRAY#[1])
:SET &VAR# = 2
:WHILE &VAR# LE 30
:IF STR_LENGTH(&EM_INFRA_PRVDR_ARRAY#[&VAR#]) <> 0
:  SET &EM_INFRA_STRING# = STR_CAT(&EM_INFRA_STRING# ,'" , "')
:  SET &EM_INFRA_STRING# = STR_CAT(&EM_INFRA_STRING# ,&EM_INFRA_PRVDR_ARRAY#[&VAR#])
:ENDIF
:SET &VAR# = &VAR# + 1
:ENDWHILE
:SET &EM_INFRA_PRVDR#=STR_CAT(&EM_INFRA_STRING# , '"')

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_REPO_NAME#
:INC PCK.AUTOMIC_INFRASTRUCTURE_MANAGER.PUB.INCLUDE.ESCAPE_ARG_VALUE
:SET &EM_REPO_NAME#=&UC4RB_ESCAPED_STRING#

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_REPO_FOLDER#
:INC PCK.AUTOMIC_INFRASTRUCTURE_MANAGER.PUB.INCLUDE.ESCAPE_ARG_VALUE
:SET &EM_REPO_FOLDER#=&UC4RB_ESCAPED_STRING#

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_REPO_REF#
:INC PCK.AUTOMIC_INFRASTRUCTURE_MANAGER.PUB.INCLUDE.ESCAPE_ARG_VALUE
:SET &EM_REPO_REF#=&UC4RB_ESCAPED_STRING#

:SET &UC4RB_ESCAPED_STRING#=&UC4RB_EM_CREATED_BY#
:INC PCK.AUTOMIC_INFRASTRUCTURE_MANAGER.PUB.INCLUDE.ESCAPE_ARG_VALUE
:SET &EM_CREATED_BY#=&UC4RB_ESCAPED_STRING#

:IF &UC4RB_EM_CREATED_BY# <> ""
:   SET &EM_CREATED_BY#=STR_CAT(',"requester":"',&UC4RB_ESCAPED_STRING#)
:   SET &EM_CREATED_BY#=STR_CAT(&EM_CREATED_BY#,'"')
:ENDIF
]]></pre_process>
      </row>
