         <type>SQL</type>
         <sql><![CDATA[SELECT
 pg_database.datname,
 pg_size_pretty(pg_database_size(pg_database.datname)) AS size
 FROM pg_database
where
pg_database.datname like '%'
order by pg_database_size(pg_database.datname) desc]]></sql>
