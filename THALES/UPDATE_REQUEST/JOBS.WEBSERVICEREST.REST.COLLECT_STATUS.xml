   <scripts>
      <row>
         <pre_process><![CDATA[!:set &SERVICE_NOW_REPLY_FILE# = get_var(VARA.RBA.CONFIG,SERVICE_NOW_REPLY_FILE,1)
!
!:set &SERVICE_NOW_REPLY_LOGIN# = get_var(VARA.RBA.CONFIG,SERVICE_NOW_REPLY_LOGIN,1)
!:set &SERVICE_NOW_REPLY_FILE# = get_var(VARA.RBA.CONFIG,SERVICE_NOW_REPLY_PATH,1)
!:set &SERVICE_NOW_REPLY_FILE# = str_cat(&SERVICE_NOW_REPLY_FILE#,"service_now_reply_")
!:set &SERVICE_NOW_REPLY_FILE# = str_cat(&SERVICE_NOW_REPLY_FILE#,&METAFLOW_RUNID#)
!:pset &SERVICE_NOW_REPLY_FILE# = str_cat(&SERVICE_NOW_REPLY_FILE#,".json")

!:set &METAFLOW_RUNID#=0001021292]]></pre_process>
      </row>
      <row>
         <post_process><![CDATA[:SET &COUNTER# = 0
!:set &METAFLOW_OK# = "N"
!:set &TECHNICAL_FLOW# = "UNKNOWN"
!:set &ENRICH# = "UNKNOWN"
!:set &UPDATE_REQUEST# = "UNKNOWN"

:SET &HND_PP# = PREP_PROCESS_REPORT(, , 'REP')
:PROCESS &HND_PP#
:  SET &LINE#=GET_PROCESS_LINE(&HND_PP#)
:  SET &COUNTER# = ADD(&COUNTER#, 1)
:  set &POS# = STR_FIND(&LINE#, "Status:200 OK")
:  if &POS# > 0
:    if &METAFLOW_OK# = "N"
:      pset &METAFLOW_OK# = "DONE"
:    endif
:  endif

:  set &POS# = STR_FIND(&LINE#, "JOBP.TECHNICAL_FLOW")
:  if &POS# > 0
:    if &TECHNICAL_FLOW# = "UNKNOWN"
:      pset &TECHNICAL_FLOW# = "TO_CHECK"
:    endif
:  endif
:  set &POS# = STR_FIND(&LINE#, "status_text")
:  if &POS# > 0
:    if &TECHNICAL_FLOW# = "TO_CHECK"
:      set &POS# = STR_FIND(&LINE#, "ENDED_OK - ended normally")
:      if &POS# > 0
:        pset &TECHNICAL_FLOW# = "DONE"
:      else
:        pset &TECHNICAL_FLOW# = "FAILED"
:      endif
:    endif
:  endif

:  set &POS# = STR_FIND(&LINE#, "JOBP.ENRICH")
:  if &POS# > 0
:    if &ENRICH# = "UNKNOWN"
:      pset &ENRICH# = "TO_CHECK"
:    endif
:  endif
:  set &POS# = STR_FIND(&LINE#, "start_time")
:  if &POS# > 0
:    if &ENRICH# = "TO_CHECK"
:      set &POS# = ADD(&POS#, 14)
:      set &TEMP# = str_cut(&LINE#, &POS#, 19)
:      pset &ENRICH# = str_sub(&TEMP#, "T", " ")
:    endif
:  endif

:  set &POS# = STR_FIND(&LINE#, "JOBP.UPDATE_REQUEST")
:  if &POS# > 0
:    if &UPDATE_REQUEST# = "UNKNOWN"
:      pset &UPDATE_REQUEST# = "TO_CHECK"
:    endif
:  endif
:  set &POS# = STR_FIND(&LINE#, "start_time")
:  if &POS# > 0
:    if &UPDATE_REQUEST# = "TO_CHECK"
:      set &POS# = ADD(&POS#, 14)
:      set &TEMP# = str_cut(&LINE#, &POS#, 19)
:      pset &UPDATE_REQUEST# = str_sub(&TEMP#, "T", " ")
:    endif
:  endif

:ENDPROCESS
:CLOSE_PROCESS &HND_PP#

!:send_msg "&$USER#", "&$DEPARTMENT#", "&METAFLOW_OK#"
! :send_msg "&$USER#","&$DEPARTMENT#","&TECHNICAL_FLOW#"
! :send_msg "&$USER#","&$DEPARTMENT#","&ENRICH#"
! :send_msg "&$USER#","&$DEPARTMENT#","&UPDATE_REQUEST#"

!:if &METAFLOW_OK# <> "DONE"
!:  exit 1
!:endif]]></post_process>
      </row>
   </scripts>
