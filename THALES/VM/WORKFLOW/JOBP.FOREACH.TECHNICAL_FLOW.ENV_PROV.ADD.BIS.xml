   <scripts>
      <row>
         <process><![CDATA[!:put_var VARA.TECHNICAL_FLOW.VM.1, "1", "Mercury", "Venus", "Earth", "Mars", "Jupiter"
!:put_var VARA.TECHNICAL_FLOW.VM.1, "2", "Mercury", "Venus", "Earth", "Mars", "Jupiter"

:DEFINE &T_HOSTNAME#, string, 100
:FILL &T_HOSTNAME#[] = STR_SPLIT(&LISTE_HOSTNAME#," ")


:delete_var VARA.TECHNICAL_FLOW.VM.1
:delete_var VARA.TECHNICAL_FLOW.VM.2

:set &VM_NUMBER#=0

:include JOBI.JASON_FILE.ARRAY
:FILL &STRINGS#[] = STR_SPLIT(&TEXT01#,",")

:SET &VAR# = 1
:SET &LEN# = LENGTH(&STRINGS#[])
:set &KEY# = "1"

:WHILE &VAR# LT &LEN#
:IF &STRINGS#[&VAR#] = ""
:   SET &VAR# = &LEN#
:ELSE
:   if str_find(&STRINGS#[&VAR#],"virtualMachine") > 0
:      set &VM_NUMBER#= &VM_NUMBER# + 1
:   endif
:   if str_find(&STRINGS#[&VAR#],"id=") > 0
:      set &ID# = &STRINGS#[&VAR#]
:   endif
:   if str_find(&STRINGS#[&VAR#],"status=") > 0
:      set &STATUS# = &STRINGS#[&VAR#]
:   endif
:   if str_find(&STRINGS#[&VAR#],"os=") > 0
:      set &OS# = &STRINGS#[&VAR#]
:   endif
:   if str_find(&STRINGS#[&VAR#],"description=") > 0
:      set &DESC# = &STRINGS#[&VAR#]
:   endif
:   if str_find(&STRINGS#[&VAR#],"sla=") > 0
:      set &SLA# = &STRINGS#[&VAR#]
:      if &VM_NUMBER# > 0
:          put_var VARA.TECHNICAL_FLOW.VM.1,&VM_NUMBER#,&ID#,&STATUS#,&DESC#,&OS#,&SLA#
:      endif
:   endif
:   if str_find(&STRINGS#[&VAR#],"vpu=") > 0
:      set &VPU# = &STRINGS#[&VAR#]
:   endif
:   if str_find(&STRINGS#[&VAR#],"VMemory=") > 0
:      set &VMEMORY# = &STRINGS#[&VAR#]
:   endif
:   if str_find(&STRINGS#[&VAR#],"allocated=") > 0
:      set &VMEMORY_ALLOCATED# = &STRINGS#[&VAR#]
:      if &VM_NUMBER# > 0
:          put_var VARA.TECHNICAL_FLOW.VM.2,&VM_NUMBER#,&VPU#,&VMEMORY#,&VMEMORY_ALLOCATED#
:      endif
:   endif
:   set &VAR# = &VAR# + 1
:endif
:ENDWHILE
]]></process>
      </row>
