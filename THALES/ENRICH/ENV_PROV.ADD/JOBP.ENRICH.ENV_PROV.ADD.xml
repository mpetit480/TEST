   <scripts>
      <row>
         <process><![CDATA[!:set &VM_NUMBER#=0
!:set &VARA_NAME_ROOT# = str_cat("VARA.VM.",&METAFLOW_RUNID#)
!!:set &NUM# = format(&VM_NUMBER#,"000")
!
!!:DEFINE &STRINGS#, string, 500
!:include JOBI.JASON_FILE.ARRAY
!:FILL &STRINGS#[] = STR_SPLIT(&TEXT01#,",")
!
!:SET &VAR# = 1
!:SET &LEN# = LENGTH(&STRINGS#[])
!!:send_msg AUTOMIC,AUTOMIC,&LEN#
!
!:WHILE &VAR# LT &LEN#
!!:SET &VAR# = FORMAT(&VAR#,"0")
!!:send_msg AUTOMIC,AUTOMIC,"&VAR# : &STRINGS#[&VAR#]"
!:IF &STRINGS#[&VAR#] = ""
!:   SET &VAR# = &LEN#
!:ELSE
!:if str_find(&STRINGS#[&VAR#],"virtualMachine") > 0
!:   set &VM_NUMBER#=&VM_NUMBER# + 1
!:   set &VARA_NAME# = str_cat(&VARA_NAME_ROOT#,&VM_NUMBER#)
!!:   send_msg AUTOMIC,AUTOMIC,&VARA_NAME#
!:   include JOBI.CREATE.VARA.VM
!:endif
!
!:set &POS# = str_find(&STRINGS#[&VAR#],"os=")
!:if &POS# > 0
!:   set &POS# = &POS# + 3
!!:   send_msg AUTOMIC,AUTOMIC,str_cut(&STRINGS#[&VAR#],&POS#)
!:   set &OS# = str_cut(&STRINGS#[&VAR#],&POS#)
!:   put_var_col &VARA_NAME#,"0000000000000008",2,"&OS#"
!:endif
!
!:set &POS# = str_find(&STRINGS#[&VAR#],"vpu=")
!:if &POS# > 0
!:   set &POS# = &POS# + 10
!:   set &LEN1# = str_lng(&STRINGS#[&VAR#])
!:   set &LEN1# = &LEN1# - &POS#
!!:   send_msg AUTOMIC,AUTOMIC,str_cut(&STRINGS#[&VAR#],&POS#,&LEN1#)
!!:   send_msg AUTOMIC,AUTOMIC,"&STRINGS#[&VAR#],&POS#,&LEN1#"
!:   set &USED# = str_cut(&STRINGS#[&VAR#],&POS#,&LEN1#)
!:   put_var_col &VARA_NAME#,"0000000000000010",2,"&USED#"
!:endif
!
!:   SET &VAR# = &VAR# + 1
!:ENDIF
!:ENDWHILE
!
!]]></process>
      </row>
