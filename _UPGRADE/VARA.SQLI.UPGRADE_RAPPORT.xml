         <type>SQLI</type>
         <sql><![CDATA[select 
O.OH_Name,
O.OH_OTYPE,
O.OH_ModDate, 
H.HOST_Version,
 H.HOST_PathBin, 
H.HOST_TargetVersion, 
H.HOST_UpgradeRunId, 
O.OH_DeleteFlag 
from 
  OH O join HOST H on O.OH_Idnr=H.HOST_OH_Idnr 
where 
H.HOST_Version is not NULL 
and H.HOST_HostAttrType in ('UNIX','WINDOWS','SQL') 
and H.HOST_PathBin>' ' order by OH_NAME]]></sql>
